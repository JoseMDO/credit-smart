
<% if favorite %>
  <%= link_to favorite, data: { turbo_method: :delete }, class: "btn btn-danger star-button", title: "Unfavorite" do %>
    <i class="fas fa-star filled"></i>
  <% end %>
<% else %>
  <%= render "favorites/form", favorite: Favorite.new, credit_card_id: credit_card.id %>
<% end %>
